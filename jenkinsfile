pipeline {
    agent any

    //addeed a stage
    stages {
        //
        stage("Build") {
            steps {
                sh "mvn clean package"
            }
        }
        //this is 2
        //this is last last
        stage("Save Artifact") {
            steps {
                archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false
            }
        }
    }
}
